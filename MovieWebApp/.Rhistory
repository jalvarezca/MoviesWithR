require(data.table)
require(dplyr)
require(DT)
require(readr)
require(ggplot2)
## Data Source Variables
DocName <- "movie_metadata.csv"       # Document Name with extension
MyData <- paste0("./data/",DocName)   # Path for Data Source
MoviesDf <- read.csv(MyData)
#Select only needed columns
t1 <- select(
.data = MoviesDf,
director_name,
gross,
genres,
movie_title,
budget,
country,
title_year
)
#filter, remove blank director names
t2 <- filter(
.data = t1,
director_name != ""
)
#mutate, create new column profit
t3 <- mutate(
.data = t2,
profit = gross-budget
)
t3
head(t3)
#mutate, create new column profit
t3 <- mutate(
.data = t2,
profit = as.numeric(gross)-as.numeric(budget)
)
head(t3)
#mutate, create new column profit
t3 <- mutate(
.data = t2,
profit = as.numeric(gsub('[$,]', '', gross)) -
as.numeric(gsub('[$,]', '', budget))
)
head(t3)
#group by director name
t4 <- group_by(
.data = t3,
director_name
)
head(t4)
t4
#Aggregate base on groups
t5 <- summarize(
.data = t4,
TotalMovies = count(director_name)
)
#Aggregate base on groups
t5 <- summarize(
.data = t4,
count =n())
#Aggregate base on groups
t5 <- summarize(
.data = t4,
count =n())
t5
#Aggregate base on groups
